[
    {
        "channels": "/chat/**",
        "filter"  : "org.cometd.server.filter.NoMarkupFilter",
        "init"    : {}
    },
    {
        "channels": "/chat/*",
        "filter"   : "org.cometd.server.filter.RegexFilter",
        "init"    : [
            [ "[fF].ck","dang" ],
            [ "teh ","the "]
        ]
    },
    {
        "channels": "/chat/**",
        "filter"   : "org.cometd.server.filter.RegexFilter",
        "init"    : [
            [ "[Mm]icrosoft", "Micro\\$oft" ],
            [ ".*tomcat.*", null ]
        ]
    }
]
